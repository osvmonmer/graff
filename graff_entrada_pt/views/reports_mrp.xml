<odoo>
    <data>
        <record model="ir.ui.view" id="reports_mrp_tree_view">
            <field name="name">reports_mrp_tree_view</field>
            <field name="model">reports.mrp</field>
            <field name="arch" type="xml">
                <tree string="Branch state">
                    <field name="date" />
                    <field name="cantidad" />
                    <field name="no_client" />
                    <field name="clave" />
                    <field name="product_id"/>
                    <field name="op_id" />
                    <field name="res_partner_id" />
                    <field name="pesouni" />  
                    <field name="peso" />  
                    <field name="estado" />
                    <field name="pv" />
                    <field name="importe" sum="importe"/>
                </tree>
            </field>
        </record>      
    </data>
</odoo>
